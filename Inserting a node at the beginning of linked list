//linkedlist:inserting a node at beginning//

#include <stdio.h>
#include <stdlib.h>
struct node
{
 int data;
 struct node*next;
};
struct node*head; //global variable
void insert(int x);
{
 struct node*temp=(node*)malloc(sizeof(struct node));
 temp->data=x;
 temp->next=head;
 head=temp;
}
void printf();
{
 struct node*temp=head;
 printf("list is:");
 while(temp!=null)
{
 printf("%d",temp->data);
 temp=temp->next;
}
 printf("\n");
}
 int main()
{
 node*head=null; //empty list
 printf("how many numbers\n");
 int n,i,x;
 scanf("%d,&n);
 for(i=0;i<n;i++)
{
 printf("enter the number\n");
 scanf("%d",&x);
 insert(x);
 print()
 }
}
